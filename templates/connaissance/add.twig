{% extends "base.html.twig" %}

{% block title %}Connaissance{% endblock title %}

{% block body %}

	<ol class="breadcrumb">
		<li><a href="{{ path('homepage') }}">Accueil</a></li>
		<li><a href="{{ path('connaissance.list') }}">Liste des connaissance</a></li>
		<li class="active">Ajouter une connaissance</li>
	</ol>
	
	<div class="well bs-component">
		{{ form_start(form, {"attr": {"action": path('connaissance.add') }}) }}
			<fieldset>
				<legend>Ajout d'une connaissance</legend>
					
					{{ form(form) }}
			</fieldset>
		{{ form_end(form) }}
	</div>
				
{% endblock body %}