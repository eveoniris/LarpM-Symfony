{% extends "base.html.twig" %}

{% block title %}Connaissances{% endblock title %}

{% block body %}

    {% include '_partials/breadcrumb.twig'
        with {
        'items' : [
            {'route': path('connaissance.list'), 'name' : 'Liste des connaissances'},
