{% extends "base.html.twig" %}

{% block title %}Background{% endblock title %}

{% block body %}

	<ol class="breadcrumb">
		<li><a href="{{ path('homepage') }}">Accueil</a></li>
		<li><a href="{{ path('background.list') }}">Liste des background</a></li>
		<li class="active">Modification d'un background</li>
	</ol>

	<div class="well bs-component">	
		{{ form_start(form, {"attr": {"action": path('background.update', {'background': background.id}) }}" method="POST" {{ form_enctype(form) }}  novalidate>
			<fieldset>
				<legend>Modification d'un background</legend>
				
	
				{{ form(form) }}
			</fieldset>
		{{ form_end(form) }}
	</div>
	
{% endblock body %}