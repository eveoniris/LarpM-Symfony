						<fieldset>
							<legend>Choisissez les ingr√©dients fournis par {{ territoire.nom }}</legend>
							{% form_theme form 'Form/bootstrap_3_layout.html.twig' %}
							{{ form(form) }}
						</fieldset>
					</form>
					
					
				</div>
			</div>
		</div>
	</div>
	
{% endblock content %}