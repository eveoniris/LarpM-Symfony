{% extends "layout.twig" %}

{% block content %}


	<ol class="breadcrumb">
		<li><a href="{{ path('homepage') }}">Accueil</a></li>
		<li><a href="{{ path("territoire.admin.list") }}">Liste des territoires</a></li>
		<li><a href="{{ path("territoire.admin.detail", {'territoire': territoire.id}) }}">{{ territoire.nom }}</a></li>
