{% extends "layout.twig" %}

{% block title %}Construction{% endblock title %}

{% block content %}

	<ol class="breadcrumb">
		<li><a href="{{ path('homepage') }}">Accueil</a></li>
		<li><a href="{{ path('culture') }}">Liste des cultures</a></li>
		<li class="active">Modification d'une culture</li>
	</ol>
	
	
	<div class="well bs-component">
		<form action="{{ path('culture.update', {'culture': culture.id}) }}" method="POST" {{ form_enctype(form) }} novalidate>
			<fieldset>
				<legend>Modification d'une culture</legend>
					{% form_theme form 'Form/bootstrap_3_layout.html.twig' %}
					{{ form(form) }}
			</fieldset>
		</form>
	</div>
				
{% endblock content %}