<!DOCTYPE html>
<html lang="fr">
    <head>
        {% block head %}
            <meta charset="utf-8">
        	<meta http-equiv="X-UA-Compatible" content="IE=edge">
        	<meta name="viewport" content="width=device-width, initial-scale=1">

            <title>LarpManager - {% block title %}{% endblock title %}</title>

			<link rel="shortcut icon" href="{{ app.request.basepath }}/favicon.ico" type="image/x-icon">
			<link rel="icon" href="{{ app.request.basepath }}/favicon.ico" type="image/x-icon">

            {% block stylesheets %}
                {{ encore_entry_link_tags('app') }}
            {% endblock %}

            {% block javascripts %}
                {{ encore_entry_script_tags('app') }}
                <script type="text/javascript">

                   // $( "#test" ).css( "border", "3px solid red" );
                </script>
            {% endblock %}

			{% block style %}{% endblock style %}
        {% endblock head %}
    </head>
    <body>

		{# corps de la page #}
        <div id="content" class="container-fluid">
        	{% block body %}{% endblock body %}
        </div>
    </body>
    {% block javascript %}
    {% endblock javascript %}
</html>
