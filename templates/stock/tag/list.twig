<ul class="list-group">
	{%  for tag in tags %}
		<a href="{{ path('stockTag.update',  {tag: tag.id}) }}"
		   class="list-group-item list-group-item-action d-flex justify-content-between align-items-start">
			<div class="ms-2 me-auto">
				<h4 class="list-group-item-heading">{{ tag.nom }}</h4>
			</div>
			<span class="badge text-bg-info">{{ tag.objets|length }}</span>
		</a>
	{%  endfor %}
</ul>
