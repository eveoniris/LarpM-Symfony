{% extends "base.html.twig" %}

{% block title %}Jetons{% endblock title %}

{% block body %}

	<ol class="breadcrumb">
		<li><a href="{{ path('homepage') }}">Accueil</a></li>
		<li><a href="{{ path('token.list') }}">Liste des jetons</a></li>
		<li class="active">Ajout d'un jeton</li>
	</ol>
	
	<div class="well bs-component">
		{{ form_start(form, {"attr": {"action": path('token.add') }}" method="POST" {{ form_enctype(form) }}  novalidate>
			<fieldset>
				<legend>Ajout d'un jeton</legend>
				
	
				{{ form(form) }}
			</fieldset>
		{{ form_end(form) }}
	</div>
	
{% endblock %}